@page
@model VipCon.Pages.Parceiros.DeleteModel

@{
    ViewData["Title"] = "Deletar";
    Layout = "~/Pages/_Layout.cshtml";
}

<h2>Deletar</h2>

<h3>Tem certeza que deseja deletar este Parceiro?</h3>
<div>
    <h4>Parceiro</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Parceiro.Nome)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Parceiro.Nome)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Parceiro.Link)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Parceiro.Link)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Parceiro.Imagem)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Parceiro.Imagem)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="Parceiro.Id" />
        <input type="submit" value="Deletar" class="btn btn-default" /> |
        <a asp-page="./Index">Voltar</a>
    </form>
</div>
